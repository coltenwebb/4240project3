    var a: int := 12;
    var b: int := 3;
    var c: int;
    var d: int := 0;
    var e: int;
    var f: int;
    var g: int := 1;
    var h: int := 1;
    var i: int := 10;
    var j: int := 7;
    var k: int := 12;
    var l: int := 3;
    var m: int := 2;
in begin
    c := 0 - 1;
